{
  "expo": {
    "name": "Deelbaar",
    "slug": "deelbaar",
    "version": "1.0.0",
    "scheme": "deelbaar",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.deelbaar.app"
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDqSk5m03Tz9jpBfy18UMJ3ThjNyTYCEDk"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.deelbaar.app",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Deelbaar to use your location."
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/splash.png",
          "resizeMode": "cover"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "de8a44a5-2432-4b69-8dcb-22b1ef299d63"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/de8a44a5-2432-4b69-8dcb-22b1ef299d63"
    }
  }
}