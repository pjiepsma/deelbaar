{
  "cli": {
    "version": ">= 12.3.0",
    "appVersionSource": "remote"
  },
  "build": {
    "ios": {
      "simulator": {
        "buildType": "debug",
        "distribution": "internal",
        "device": false
      }
    },
    "android":{
      "development": {
        "developmentClient": true,
        "distribution": "internal",
        "channel": "development"
      },
      "production": {
        "autoIncrement": true,
        "channel": "production",
        "distribution": "store"
      }
    }
  },
  "submit": {
    "internal": {
      "android": {
        "track": "internal"
      }
    },
    "production": {
      "android": {
        "track": "production",
        "changesNotSentForReview": true
      }
    }
  }
}